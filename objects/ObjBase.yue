draw = require "../utils/UtilDraw"

-- Class for base object
class ObjBase
    -- Constructor
    new:(posX, posY, width, height, style) =>
        @stPos = {posX, posY}
        @enPos = {posX, posY}
        @size = {width, height}
        @style = style
        @events = {}
    -- Displaying object
    render: =>
        draw.drawRectangle @stPos[1], @stPos[2], @enPos[1], @enPos[2], @style
    -- event:(eventName, eventArgs) =>

obj = ObjBase 1, 1, 10, 10, "red"
obj\render